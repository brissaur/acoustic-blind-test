{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": [true, "log", "error"],
    "variable-name": false
  }
}
